{
  "name": "cypress-repeat-pro",
  "version": "1.0.0",
  "description": "Run Cypress multiple times in a row with pro options",
  "bin": {
    "cypress-repeat-pro": "index.js"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "install:cypress": "cypress install",
    "test:cypress": "cypress run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "demo:twice": "node . run -n 2",
    "demo:until-passes": "node . run -n 2 --until-passes",
    "demo:rerun-failed-only": "node . run -n 2 --rerun-failed-only",
    "demo:force": "node . run -n 2 --force",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagar-qa007/cypress-repeat-pro.git"
  },
  "keywords": [
    "cypress",
    "cli"
  ],
  "author": "Sagar K <tester.sagar.007@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sagar-qa007/cypress-repeat-pro/issues"
  },
  "homepage": "https://github.com/sagar-qa007/cypress-repeat-pro#readme",
  "devDependencies": {
    "@types/node": "14.10.1",
    "cypress": "^13.13.3",
    "prettier": "2.1.1",
    "semantic-release": "^22.0.5"
  },
  "dependencies": {
    "arg": "5.0.2",
    "bluebird": "3.7.2",
    "debug": "4.3.5",
    "dotenv": "8.2.0"
  },
  "peerDependencies": {
    "cypress": ">=5.3.0"
  },
  "release": {
    "branches": "main"
  }
}
